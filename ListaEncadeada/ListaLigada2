from Node import Node
class LinkedList:
    def __init__(self):
        self.head = None
        self.tail = None

    def append(self, data):
        new_node = Node(data)
        if self.head is None:
            self.head = new_node
        else:
            self.tail.next = new_node
        self.tail = new_node

    def print_list(self):
        current = self.head
        while current is not None:
            print(current.data)
            current = current.next


lista = LinkedList()



lista.append(2)
lista.append(3)
lista.append(20)
lista.append(21)
lista.append(26)
lista.append(243)

ponteiro2 = lista.head

while(ponteiro2):
    print(ponteiro2.data)
    ponteiro2 = ponteiro2.next
